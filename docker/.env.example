# Docker image tag
AUTOBYTEUS_IMAGE_TAG=latest

# Host-exposed server port (maps to container 8000)
AUTOBYTEUS_BACKEND_PORT=8001

# Host-exposed VNC ports (mapped to container 5900/6080/9223)
AUTOBYTEUS_VNC_PORT=5908
AUTOBYTEUS_WEB_VNC_PORT=6088
AUTOBYTEUS_CHROME_DEBUG_PORT=9228

# Public base URL required by autobyteus-server-ts
AUTOBYTEUS_SERVER_HOST=http://localhost:8001

# VNC hosts reported through server settings (used by autobyteus-web VNC viewer).
# Use comma-separated host:port WebSocket endpoints (6088), not raw VNC TCP ports (5908).
AUTOBYTEUS_VNC_SERVER_HOSTS=localhost:6088

# Git bootstrap mode: pat or ssh
AUTOBYTEUS_GIT_AUTH_MODE=pat

# PAT mode credentials
GITHUB_USERNAME=x-access-token
GITHUB_EMAIL=autobyteus-bot@autobyteus.dev
GITHUB_PAT=

# Repository org and refs
AUTOBYTEUS_GITHUB_ORG=AutoByteus
AUTOBYTEUS_REF=main
AUTOBYTEUS_SERVER_REF=main
AUTOBYTEUS_TS_REF=main
AUTOBYTEUS_REPOSITORY_PRISMA_REF=main

# Optional explicit repository URLs (leave blank to use org/repo defaults)
AUTOBYTEUS_SERVER_TS_REPO_URL=
AUTOBYTEUS_TS_REPO_URL=
AUTOBYTEUS_REPOSITORY_PRISMA_REPO_URL=

# Runtime settings written into data-dir .env (if missing)
APP_ENV=production
PERSISTENCE_PROVIDER=sqlite
DB_TYPE=sqlite
LOG_LEVEL=INFO

# If set to 1, existing local clones are not updated on startup
AUTOBYTEUS_SKIP_SYNC=0
